<Page x:Class="Common.Controls.TypePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:appShared="clr-namespace:AppShared;assembly=AppShared"
      xmlns:wpfControlLibrary1="clr-namespace:WpfControlLibrary1"
      mc:Ignorable="d"
      Background="AliceBlue"
      d:DesignHeight="200" d:DesignWidth="300"
      Title="TypePage">

    <DockPanel LastChildFill="True">
        <wpfControlLibrary1:TypeControl Detailed="true">
            <wpfControlLibrary1:App.RenderedType>
                <x:Type TypeName="Window" />
            </wpfControlLibrary1:App.RenderedType>
        </wpfControlLibrary1:TypeControl>
    </DockPanel>
</Page>
