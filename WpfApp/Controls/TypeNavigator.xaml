<UserControl Name="userControl" x:Class="WpfControlLibrary1.TypeNavigator"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Common.Controls"
             xmlns:appShared="clr-namespace:AppShared;assembly=AppShared"
             xmlns:system="clr-namespace:System;assembly=mscorlib"
             xmlns:controls="clr-namespace:Controls"
             xmlns:wpfControlLibrary1="clr-namespace:WpfControlLibrary1"
             mc:Ignorable="d" 
             d:DesignHeight="180" d:DesignWidth="300">
    <DockPanel>
        <wpfControlLibrary1:TypeControl Detailed="False" Target="{Binding ElementName=TypeNavigationFrame}"  DockPanel.Dock="Top">
            <wpfControlLibrary1:App.RenderedType><Binding ElementName="userControl" Path="(wpfControlLibrary1:App.RenderedType)" FallbackValue="{x:Type system:String}"/></wpfControlLibrary1:App.RenderedType>
        </wpfControlLibrary1:TypeControl>
        <Frame Name="TypeNavigationFrame" Source="../Controls/DefaultPage.xaml"></Frame>
    </DockPanel>
</UserControl>
